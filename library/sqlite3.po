msgid "``INTEGER``"
msgstr "``INTEGER``"

#: ../Doc/library/sqlite3.rst:1751 ../Doc/library/sqlite3.rst:1768
msgid ":class:`float`"
msgstr ":class:`float`"

#: ../Doc/library/sqlite3.rst:1751 ../Doc/library/sqlite3.rst:1768
msgid "``REAL``"
msgstr "``REAL``"

#: ../Doc/library/sqlite3.rst:1753
msgid ":class:`str`"
msgstr ":class:`str`"

#: ../Doc/library/sqlite3.rst:1753 ../Doc/library/sqlite3.rst:1770
msgid "``TEXT``"
msgstr "``TEXT``"

#: ../Doc/library/sqlite3.rst:1755 ../Doc/library/sqlite3.rst:1773
msgid ":class:`bytes`"
msgstr ":class:`bytes`"

